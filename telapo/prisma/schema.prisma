datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider        = "prisma-client-js"
    previewFeatures = []
}

model User {
    id          String  @id @default(uuid()) @db.Uuid
    email       String  @unique
    firstName   String
    lastName    String
    familyGroup String
    passhash    String
    drawsId     String? @unique @db.Uuid
    draws       User?   @relation("UserDraws", fields: [drawsId], references: [id])
    drawnBy     User[]  @relation("UserDraws")
}
